#include <iostream>
#include <vector>
#include <string>
#include <algorithm>
#define FOR(i, j, k, in) for (int i=j; i < k; i+= in)
#define REP(i, n) FOR(i, 1, n, 1)

using namespace std;

int main() {
        string dec;
        cin >> dec;
        int final[100];
        int n = dec.length();
        for(int i = 0; i <= dec.length(); i++) {
                final[i] = i + 1;
        }
        for(int i = 0; i <= n; i++) {
                for(int j = 0; j <= n - i; j++) {
                        if( dec[i] == 'D') {
                                if(final[i+1] > final[i]  ) {
                                        swap(final[i], final[i+1]);
                                }
                        }
                        else if( dec[i] == 'I') {
                                if(final[i+1] < final[i]  ) {
                                        swap(final[i], final[i+1]);
                                }
                        }
                }
        }
    FOR(i, 0, n+1, 1) {
        cout << final[i];
    }
}                                                                                                                                                             
